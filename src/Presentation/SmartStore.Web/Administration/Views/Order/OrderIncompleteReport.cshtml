@model IList<OrderIncompleteReportLineModel>
@using Telerik.Web.Mvc;
@using Telerik.Web.Mvc.UI

<div class="widget widget-table">
    
    <div class="widget-header">
        <i class="fa fa-signal"></i>
        <h3>@T("Admin.SalesReport.Incomplete")</h3>
    </div>
    
	<div class="widget-content equalized-column">
		<table class="table table-striped table-hover">
			<thead>
				<tr>
					<th>@T("Admin.SalesReport.Incomplete.Item")</th>
					<th>@T("Admin.SalesReport.Incomplete.Total")</th>
					<th>@T("Admin.SalesReport.Incomplete.Count")</th>
				</tr>
			</thead>
			<tbody>
				@foreach (var x in Model)
				{
					<tr>
						<td>
							@if (x.Url.HasValue())
							{
								<a href="@x.Url">@x.Item</a>
							}
							else
							{
								@x.Item
							}
						</td>
						<td>@x.Total</td>
						<td>@x.Count</td>
					</tr>
				}
				<tr>
					<td>&nbsp;</td>
					<td>&nbsp;</td>
					<td>&nbsp;</td>
				</tr>
			</tbody>
		</table>
	</div>



</div>

