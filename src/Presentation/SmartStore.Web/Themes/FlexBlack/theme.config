<?xml version="1.0" encoding="utf-8" ?>
<Theme baseTheme="Flex" title="Flex Black" previewText="'Flex Black' default theme" author="SmartStore AG" version="1.0">

  <Vars>
    <Var name="indigo" type="Color">#3f51b5</Var>
    <Var name="boxed" type="Boolean">true</Var>
    <Var name="link-color" type="Color">$body-color</Var>
    <Var name="inverse-footer" type="Boolean">true</Var>
    <Var name="footer-bg" type="Color">#464646</Var>
	  <Var name="btn-primary-bg" type="Color">#666666</Var>
	  <Var name="btn-primary-border" type="Color">#444444</Var>
