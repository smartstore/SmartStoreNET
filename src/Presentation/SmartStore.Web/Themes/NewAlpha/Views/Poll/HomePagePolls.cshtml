@model IList<PollModel>
@using SmartStore.Web.Models.Polls;
@if (Model.Count > 0)
{
    <div class="todays-poll-box well well-small">
        <div class="todays-poll-box-block clearfix">
            @foreach (var poll in Model)
            { 
                @Html.Partial("_Poll", poll)
            }
        </div>
    </div>
}
