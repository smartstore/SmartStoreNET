@model ForumGroupModel
@using SmartStore.Web.Models.Boards;
@{
    Layout = "_ColumnsOne";

    Html.AddTitleParts(Model.Name);
}
<div class="page forum-group-page">
    @{
        Html.RenderAction("ForumBreadcrumb", new { forumGroupId = Model.Id });
        Html.RenderPartial("_SearchBox");
    }

    <div class="forum-group">
        @{ Html.RenderPartial("_ForumGroupPartial", Model); }
    </div>
</div>
